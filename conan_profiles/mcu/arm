include(default)

# Note that I assume that you use a custom settings.yml where you add
# an os key to distinguish between different Mcus and normal os
[settings]
os=Mcu
arch=armv7
compiler=gcc
compiler.version=10
compiler.libcxx=libstdc++11
build_type=Release
[options]
[build_requires]
*: arm.toolchain.compiler/1.0.0
*: arm.toolchain.flags/1.0.0
